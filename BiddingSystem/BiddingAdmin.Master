    <script type="text/javascript">
        var mainTab = <%= getJsonMainTab()%>;
        var subTitle =<%= getJsonSubTab()%>;
        var subTabId =<%= getJsonSubTabId()%>;
        var subTabVal =<%= getJsonSubTabValue()%>;
        $('#navTabNavi a[href="' + mainTab + '"]').tab('show');
        $('#' + subTabId).css("color", "rgb(60, 141, 188)");
        $('#' + subTitle + ' a[href="' + subTabVal + '"]').tab('show');

        $(function () {
            $('#imgProgress').hide();
            $('#submitPassword').on({

                click: function () {
                    if ($('#txtOldPassword').val() != "") {
                        if ($('#txtPassword').val() != "") {
                            if ($('#txtConfirmPassword').val() != "") {
                                if ($('#txtPassword').val() == $('#txtConfirmPassword').val()) {
                                    $('#imgProgress').show();
                                    $.ajax({
                                        type: "POST",
                                        url: "CompanyCreateUser.aspx/ChangePassword",
                                        data: '{userName: "' + $("#userName").html() + '",password: "' + $("#txtPassword").val() + '",oldPassword: "' + $("#txtOldPassword").val() + '" }',
                                        contentType: "application/json; charset=utf-8",
                                        dataType: 'json',
                                        success: function (response) {
                                            $('#imgProgress').hide();
                                            if (response.d == "1") {
                                                $('#txtOldPassword').val("");
                                                $('#txtPassword').val("");
                                                $('#txtConfirmPassword').val("");
                                                $('#mdlPassword').modal('hide');

                                                swal({ type: 'success', title: 'SUCCESS', text: 'Your work has been saved', showConfirmButton: false, timer: 1500 });
                                            }
                                            else if (response.d == "-1") {
                                                swal({ type: 'error', title: 'ERROR', text: 'Entered Old Password Is Incorrect' });
                                            }
                                            else {
                                                swal({ type: 'error', title: 'ERROR', text: 'Error On Changing Password' });
                                            }
                                        }
                                    });


                                }
                                else {
                                    swal({ type: 'error', title: 'ERROR', text: 'Entered passwords do not match' });
                                }
                            }
                            else {
                                swal({ type: 'error', title: 'ERROR', text: 'Please Fill Confirm Password' });
                            }
                        }
                        else {
                            swal({ type: 'error', title: 'ERROR', text: 'Please Enter New Password' });
                        }
                    }
                    else {
                        swal({ type: 'error', title: 'ERROR', text: 'Please Enter Your Old Password' });
                    }
                }
            });
        });

    </script>
